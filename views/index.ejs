<h3 class="text-center">Here you can find your new home </h3>
<div id="map"></div>


<h3 class="text-center">Available Listings</h3>
<% listings.forEach((listing) => { %>
  <div class="row">
    <div class="col s6">
      <div class="card">
        <div class="card-image waves-effect waves-block waves-light">
          <img class="activator" src="public/images/listing/123/qpisos_5.jpg">
        </div>
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4">
                <a class="title"  href="/listings/<%= listing._id %>">
            <strong><%= listing.title %></strong>
          </a><br>
          <span class="light-text description"><%= listing.description %></span>
          </br>
          <i class="material-icons right">more_vert</i></span>
          <p><a href="#">This is a link</a></p>
        </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4">Card Title<i class="material-icons right">close</i></span>

        </div>
      </div>
    </div>

    <%  }) %>
  </div>



  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCKIv36asP3gBqbOWJsq4lEtYPRx1LW-Sw"></script>
  <script type="text/javascript">
    var myListings = <%- JSON.stringify(listings) %>
  </script>
  <script type="text/javascript" src="/javascripts/main.js"></script>